    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-register": "^6.16.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^9.4.7",
    "minifyify": "^7.3.3",
    "rollupify": "^0.3.4",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "glslify-sync/transform"
    ]
  }
